{
  "compilerOptions": {
    "target": "es2022",
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "test": ["test"],
      "quadbin": ["src"]
    }
  },
  "include": ["src"],
  "exclude": []
}
